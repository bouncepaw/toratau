%[cat
  [define import {#include %1}]
  [define proc {void %1 (%2) {
%3
}}]
  [define println {printf("%s\n", %1);}]]

%[import <stdio.h>]
%[proc main void
       [lines [println {"Hello world!"}]
              "return 0;"]]

%[lines
  [define foo bar]
  [define analyze-args {
Number: %#
Unwrapped: %*
Wrapped: %@}]
  [analyze-args]
  [analyze-args one two {three four} [foo]]]

